
[$ext]
type=endpoint
auth=$ext
aors=$ext
transport=transport-$transport
context=internal
rtp_symmetric=yes
force_rport=yes
rewrite_contact=yes
direct_media=no
;allow_subscribe = yes ;(default)

;moh_suggest = default ;(default)
;send_rpid = no ;(default)
send_pai = yes 
;allow_transfer = yes ;(default)
;think these next two should both be no
;trust_id_inbound = yes
;trust_id_outbound = yes

callerid = "$desc" <$ext>
named_call_group=$clst
named_pickup_group=$clst
media_encryption=sdes
media_encryption_optimistic=yes
device_state_busy_at = 0
mailboxes = $ext@$clst

allow = !all,ulaw,alaw

[$ext]
type=auth
auth_type=userpass
password=$password
username=$username
 
[$ext]
type=aor
;contact=sip:$host
max_contacts = 1
qualify_frequency=30
;authenticate_qualify = no ;(default)
remove_existing = yes ;(default no)
minimum_expiration = 60 ;(default)
default_expiration = 120
maximum_expiration = 600